{
    "manifest_version": 3,
    "name": "AutoLoansBot",
    "version": "1.0",
    "description": "Abre m√∫ltiples ventanas, una por cada hilo.",
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "activeTab",
        "windows",
        "webRequest",
        "webRequestBlocking",
        "cookies",
        "proxy",
        "tabs"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "worker.js"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}